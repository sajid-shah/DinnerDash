
<div>
<%= link_to "New Item", new_item_path , class: "btn btn-primary" %>
</div>
<br> <br>



<div class="d-flex justify-content-between flex-wrap">
  <% @items.each do |item| %>
    <div class="col">
      <div class="card h-100" style="width: 20em;">
        <img src="data:image/jpeg;base64,
        
        MKbTelUhn/2Q==" class="img-thumbnail" alt="...">
        <div class="card-body h-100">
          <h5 class="card-title"><%= item.title%></h5>
          <p class="card-text"><%= item.description%></p>
        </div>
        <ul class="list-group list-group-flush">
          <li class="list-group-item">Price Rs.<%= item.price%> </li>
        </ul>
        <div class="card-body">
          <%= link_to "Show", item , class: "btn btn-secondary" %>
          <%= link_to "Delete", item , class: "btn btn-danger", method: :delete %>
        </div>
      </div>
    </div>
  <% end %>
</div>
